extends: default

rules:
  braces:
    level: warning
  brackets:
    level: warning
  colons:
    level: warning
  commas:
    level: warning
  empty-lines:
    level: warning
  hyphens:
    level: warning
  line-length:
    max: 80
    level: warning
  new-line-at-end-of-file:
    level: warning
  trailing-spaces:
    level: warning
